using System;

namespace Matrix_masodik_hazi
{
    class Program
    {
        static void Main(string[] args)
        {

            // bekéri a mátrix sor és oszlopszámát szóközzel elválasztva
            string rowCol = Console.ReadLine();
            string[] rowColSplit = rowCol.Split(' ');
            int[] rcint = Array.ConvertAll(rowColSplit, int.Parse);

            int row = rcint[0];
            int col = rcint[1];

            //ide kell beírni a betőket, annyi betűt, ahány oszlopot beírt
            //és annyi sor betűt, ahányat beírt
            int sor = row;
            string[] elsoTomb = new string[row + col];
            while (sor <= row)
            {
                for (int j = 0; j < sor; j++)
                {
                    elsoTomb[j] = Console.ReadLine();
                }
                sor++;
            }
            sor = 0;

            //itt kellene az elsoTomb[]-ből a szavak betűit belerakni pl a betuk[] tömbbe
            //row= sor amennyit beirt
            //col=oszlopszám,   col*row= összes betű, ennyi elem kell char[] betuk-be
            char[] betuk = new char[col];
            char[] ujBetuk = new char[row * col];
            for (int i = 0; i < elsoTomb.Length; i++)
            {
                string szo = elsoTomb[i];
                for (int j = 0; j < col; j++)
                {
                    betuk = szo.ToCharArray();
                }
            }

            //for (int i = 0; i < row*col; i++)
            //{
            //    Console.Write(ujBetuk[i] + "\t");
            //}




        }
